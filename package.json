{
  "name": "timelapse",
  "version": "0.0.1",
  "description": "App for taking timelapse videos via your desktop camera.",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelwschultz/timelapse-app.git"
  },
  "license": "MIT",
  "author": {
    "name": "Michael Schultz",
    "email": "michaelwschultz@gmail.com",
    "url": "https://github.com/michaelwschultz"
  },
  "main": "src/electron.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "dev": "cross-env NODE_ENV=development webpack serve --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "dev:nocam": "yarn run dev --env=nocam",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "package": "yarn run build",
    "postpackage": "electron-packager ./ --out=./builds"
  },
  "engines": {
    "node": ">=9.0.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "electron-is-dev": "^1.2.0",
    "ffbinaries": "^1.1.4",
    "fluent-ffmpeg": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "videoshow": "github:michaelwschultz/videoshow"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "babili-webpack-plugin": "^0.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "electron": "^10.1.3",
    "electron-builder": "^22.6.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^15.1.0",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "mini-css-extract-plugin": "^1.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  }
}